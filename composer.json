{
    "name": "pharaonic/laravel-short-url",
    "description": "Laravel - Short URL Generator { URL, Route }.",
    "keywords": [
        "laravel short url",
        "php short url",
        "short url",
        "laravel",
        "php"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Moamen Eltouny (Raggi)",
            "email": "raggi@raggitech.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": ">=6.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pharaonic\\Laravel\\ShortURL\\ShortURLServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Pharaonic\\Laravel\\ShortURL\\": "src"
        },
        "files": [
            "src/Helper.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
